using static System.Runtime.InteropServices.JavaScript.JSType;

public class MainClass
{
    public static void Main()
    {
        int count = 0;


        foreach (var qa in victor)
        {
            Console.WriteLine(qa.Key);
            string ans = Console.ReadLine();
            if (ans == qa.Value)
            {
                count += 1;
            }
        }
        Console.WriteLine(dig(count));
    }
    static string dig(int a)
    {
        string[] diag = ["идиот", "кретин", "дурак", "нормальный", "талант", "гений"];
        return diag[a];
    }
